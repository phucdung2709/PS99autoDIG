local EGG_REMOTE_PATH = { "Network", "Eggs_Roll" }
local EGG_ARGS = nil

local Players            = game:GetService("Players")
local PathfindingService = game:GetService("PathfindingService")
local TextChatService    = game:GetService("TextChatService")
local ReplicatedStorage  = game:GetService("ReplicatedStorage")

local player  = Players.LocalPlayer
local channel = TextChatService:FindFirstChild("TextChannels")
              and TextChatService.TextChannels:FindFirstChild("RBXGeneral")

-- H√†m ch·ªù an to√†n
local function safeWait(t)
    if typeof(t) == "NumberRange" then t = t.Min end
    task.wait(t)
end

-- üó®Ô∏è Chat t·ª± ƒë·ªông (g·ª≠i ngay l·∫≠p t·ª©c, sau ƒë√≥ l·∫∑p 3 ph√∫t)
if channel then
    task.spawn(function()
        local i = 1
        while true do
            pcall(function() channel:SendAsync(CHAT_MESSAGES[i]) end)
            i = (i % #CHAT_MESSAGES) + 1
            safeWait(CHAT_DELAY)
        end
    end)
end

-- ü•ö T·ª± ƒë·ªông roll egg (roll ngay l·∫≠p t·ª©c, sau ƒë√≥ m·ªói 30s)
local function startEggRolling()
    local remote = ReplicatedStorage
    for _, name in ipairs(EGG_REMOTE_PATH) do
        remote = remote:WaitForChild(name)
    end
    task.spawn(function()
        while true do
            pcall(function()
                if EGG_ARGS then
                    remote:InvokeServer(table.unpack(EGG_ARGS))
                else
                    remote:InvokeServer()
                end
            end)
            safeWait(ROLL_DELAY)
        end
    end)
end
startEggRolling()

-- üß† Thi·∫øt l·∫≠p tu·∫ßn tra + nh·∫£y cho nh√¢n v·∫≠t
local function setupCharacter(char)
    local humanoid = char:WaitForChild("Humanoid")
    local rootPart = char:WaitForChild("HumanoidRootPart")

    local function walkTo(target)
        local path = PathfindingService:CreatePath{
            AgentRadius = 2,
            AgentHeight = 5,
            AgentCanJump = true,
        }
        path:ComputeAsync(rootPart.Position, target)
        if path.Status ~= Enum.PathStatus.Success then return end

        local blocked = false
        local conn = path.Blocked:Connect(function() blocked = true end)
        for _, wp in ipairs(path:GetWaypoints()) do
            if blocked then break end
            humanoid:MoveTo(wp.Position)
            if wp.Action == Enum.PathWaypointAction.Jump then
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
            if not humanoid.MoveToFinished:Wait() then break end
        end
        conn:Disconnect()
    end

    task.spawn(function()
        while char.Parent do
            safeWait(JUMP_DELAY)
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end)

    task.spawn(function()
        local idx = 1
        while char.Parent do
            walkTo(PATROL_POINTS[idx])
            idx = (idx % #PATROL_POINTS) + 1
            safeWait(1)
        end
    end)
end

player.CharacterAdded:Connect(setupCharacter)
if player.Character then setupCharacter(player.Character) end
