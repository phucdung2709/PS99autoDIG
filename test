local PATROL_POINTS = {
    Vector3.new(80.87, 0.42, -36.25),
    Vector3.new(95.24, 0.42, 84.61),
    Vector3.new(-118.13, 0.05, 106.55),
    Vector3.new(-83.54, 0.00, -26.32),
}

local JUMP_DELAY = 5
local CHAT_DELAY = 180
local ROLL_DELAY = 30

local CHAT_MESSAGES = {
